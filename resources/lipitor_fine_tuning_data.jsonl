or_medical_conditions': [['tore a calf muscle', 133, 151], ['coronary disease', 286, 302], ['coronary disease', 449, 465]], 'medications': [['Lipitor', 201, 208], ['Lipitor', 328, 335], ['Lipitor', 769, 776], ['Toprol', 833, 839], ['Lipitor', 898, 905]], 'clinical_findings': [['flu', 622, 625], ['fever', 634, 639]], 'symptoms_experienced_by_patients': [['low bp and pulse', 856, 872]]}}]}
